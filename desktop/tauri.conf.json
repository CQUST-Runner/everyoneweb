{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/cli/schema.json",
  "package": {
    "productName": "EveryoneWeb",
    "version": "1.0.0"
  },
  "build": {
    "distDir": "http://127.0.0.1:16224/app",
    "devPath": "http://127.0.0.1:4200",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "org.hyan23.everyoneweb-desktop",
      "icon": [
        "icons/48x48.png",
        "icons/96x96.png",
        "icons/128x128.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "../build/server"
      ],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "dialog": {
        "all": true,
        "open": true
      }
    },
    "windows": [
      {
        "title": "EveryoneWeb Desktop",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "pattern": {
      "use": "brownfield"
    }
  }
}